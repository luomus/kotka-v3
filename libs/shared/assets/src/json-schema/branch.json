  "title": "Branch",
  "description": "",
  "properties": {
    "@context": {
      "type": "string",
      "range": "xsd:string",
      "subject": "@context",
      "title": "Context for the Branch"
    },
    "id": {
      "type": "string",
      "range": "xsd:string",
      "subject": "id",
      "title": "Id for the Branch"
    },
    "@type": {
      "type": "string",
      "range": "xsd:string",
