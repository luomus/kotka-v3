{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "subject": "MM.audio",
  "additionalProperties": false,
  "title": "Audio recording",
  "description": "",
  "properties": {
    "@context": {
      "type": "string",
      "range": "xsd:string",
      "subject": "@context",
      "title": "Context for the Audio recording"
    },
    "id": {
      "type": "string",
      "range": "xsd:string",
      "subject": "id",
      "title": "Id for the Audio recording"
    },
    "@type": {
      "type": "string",
      "range": "xsd:string",
      "subject": "@type",
      "title": "Type for the Audio recording"
    },
    "LuomusIntellectualRights": {
      "type": "string",
      "enum": [
        "",
        "MM.LuomusRightsFreeToUse",
        "MM.LuomusRightsInternalOnly"
      ],
      "enumNames": [
        "",
        "Free to use and publish",
        "Internal use only"
      ],
      "range": "MM.LuomusIntellectualRightsEnum",
      "subject": "MM.LuomusIntellectualRights",
      "title": "Intellectual rights given to LUOMUS"
    },
    "caption": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MM.caption",
      "title": "Caption"
    },
    "captureDateTime": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MM.captureDateTime",
      "title": "Date taken"
    },
    "capturerVerbatim": {
      "type": "array",
      "uniqueItems": false,
      "items": {
        "type": "string"
      },
      "minItems": 0,
      "range": "xsd:string",
      "subject": "MM.capturerVerbatim",
      "title": "Photographer/recorder (Forename Surname)"
    },
    "documentURI": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MM.documentURI",
      "title": "documentURI"
    },
    "fullURL": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MM.fullURL",
      "title": "URL of full size version of the image (converted to JPG)"
    },
    "keyword": {
      "type": "array",
      "uniqueItems": false,
      "items": {
        "type": "string"
      },
      "minItems": 0,
      "range": "xsd:string",
      "subject": "MM.keyword",
      "title": "Keywords"
    },
    "mp3URL": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MM.mp3URL",
      "title": "URL of mp3 audio file (stripped original or compressed from original)"
    },
    "originalFilename": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MM.originalFilename",
      "title": "Original filename"
    },
    "sourceSystem": {
      "type": "string",
      "range": "KE.informationSystem",
      "subject": "MM.sourceSystem",
      "title": "Source information system of the multimedia object"
    },
    "taxonDescriptionCaption": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "en": {
          "type": "string"
        },
        "fi": {
          "type": "string"
        },
        "sv": {
          "type": "string"
        }
      },
      "range": "xsd:string",
      "subject": "MM.taxonDescriptionCaption",
      "title": "Caption"
    },
    "taxonURI": {
      "type": "array",
      "uniqueItems": false,
      "items": {
        "type": "string"
      },
      "minItems": 0,
      "range": "MX.taxon",
      "subject": "MM.taxonURI",
      "title": "Taxon ID"
    },
    "taxonVerbatim": {
      "type": "array",
      "uniqueItems": false,
      "items": {
        "type": "string"
      },
      "minItems": 0,
      "range": "xsd:string",
      "subject": "MM.taxonVerbatim",
      "title": "Verbatim taxon name"
    },
    "thumbnailURL": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MM.thumbnailURL",
      "title": "URL of a thumbnail version of the image (maintains aspect ratio, max. 200px height or width)"
    },
    "uploadedBy": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MM.uploadedBy",
      "title": "Uploaded by"
    },
    "wavURL": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MM.wavURL",
      "title": "URL of wav audio file (stripped original)"
    },
    "intellectualOwner": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MZ.intellectualOwner",
      "title": "Tillverkare"
    },
    "intellectualRights": {
      "type": "string",
      "enum": [
        "MZ.intellectualRightsCC-BY-SA-4.0",
        "MZ.intellectualRightsCC-BY-NC-4.0",
        "MZ.intellectualRightsCC-BY-NC-SA-4.0",
        "MZ.intellectualRightsCC-BY-4.0",
        "MZ.intellectualRightsCC0-4.0",
        "MZ.intellectualRightsODBL-1.0",
        "MZ.intellectualRightsPD",
        "MZ.intellectualRightsARR",
        "MZ.intellectualRightsCC-BY-2.0",
        "MZ.intellectualRightsCC-BY-SA-2.0",
        "MZ.intellectualRightsCC-BY-SA-2.0-DE",
        "MZ.intellectualRightsCC-BY-NC-2.0",
        "MZ.intellectualRightsCC-BY-NC-SA-2.0",
        "MZ.intellectualRightsCC-BY-NC-ND-2.0",
        "MZ.intellectualRightsCC-BY-SA-2.5",
        "MZ.intellectualRightsCC-BY-SA-2.5-SE",
        "MZ.intellectualRightsCC-BY-3.0",
        "MZ.intellectualRightsCC-BY-SA-3.0",
        "MZ.intellectualRightsCC-BY-NC-SA-3.0",
        "MZ.intellectualRightsCC-BY-ND-4.0",
        "MZ.intellectualRightsCC-BY-NC-ND-4.0"
      ],
      "enumNames": [
        "Creative Commons Attribution Share-Alike 4.0",
        "Creative Commons Attribution Noncommercial 4.0",
        "Creative Commons Attribution Noncommercial Share-Alike 4.0",
        "Creative Commons Attribution 4.0",
        "Creative Commons Zero 4.0",
        "Open Data Commons Open Database License (ODbL)",
        "Public domain",
        "All rights reserved",
        "Creative Commons Attribution 2.0",
        "Creative Commons Attribution Share-Alike 2.0",
        "Creative Commons Attribution Share-Alike 2.0 Germany",
        "Creative Commons Attribution Noncommercial 2.0",
        "Creative Commons Attribution Noncommercial Share-Alike 2.0",
        "Creative Commons Attribution Noncommercial Noderivs 2.0",
        "Creative Commons Attribution Share-Alike 2.5",
        "Creative Commons Attribution Share-Alike 2.5 Sweden",
        "Creative Commons Attribution 3.0",
        "Creative Commons Attribution Share-Alike 3.0",
        "Creative Commons Attribution Noncommercial Share-Alike 3.0",
        "Creative Commons Attribution Noderivatives 4.0",
        "Creative Commons Attribution Noncommercial Noderivatives 4.0"
      ],
      "range": "MZ.intellectualRightsEnum",
      "subject": "MZ.intellectualRights",
      "title": "License"
    },
    "publicityRestrictions": {
      "type": "string",
      "enum": [
        "",
        "MZ.publicityRestrictionsPublic",
        "MZ.publicityRestrictionsProtected",
        "MZ.publicityRestrictionsPrivate"
      ],
      "enumNames": [
        "",
        "public",
        "protected",
        "private"
      ],
      "range": "MZ.publicityRestrictionsEnum",
      "subject": "MZ.publicityRestrictions",
      "description": "PUBLIC: all data can be published; PROTECTED: exact locality is hidden (100*100km square); PRIVATE: most of the data is hidden. Empty value means same as public.",
      "title": "Publicity restrictions"
    }
  },
  "required": [
    "fullURL",
    "mp3URL",
    "originalFilename",
    "sourceSystem",
    "thumbnailURL",
    "uploadedBy",
    "intellectualOwner",
    "intellectualRights"
  ]
}